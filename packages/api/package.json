{
  "name": "@anonymouslack/api",
  "version": "2.0.0",
  "description": "Anonymouslack api",
  "scripts": {
    "dev": "concurrently yarn:sls:offline yarn:tsoa:watch",
    "sls:offline": "npx sls offline start --httpPort=3100 --lambdaPort=3102",
    "tsoa": "ts-node tsoa.ts",
    "tsoa:watch": "npx nodemon --config nodemon-tsoa.json",
    "clean": "rimraf .webpack && rimraf .serverless && rimraf src/swagger.json && rimraf src/routes.ts",
    "deploy": "yarn tsoa && sls deploy --verbose"
  },
  "dependencies": {
    "@anonymouslack/universal": "^2.0.0",
    "@slack/bolt": "^3.3.0",
    "@slack/web-api": "^6.1.0",
    "@tsoa/runtime": "^3.5.2",
    "@vendia/serverless-express": "^4.3.4",
    "await-to-js": "^3.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "generate-password": "^1.6.0",
    "generic-type-guard": "^3.4.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.6",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.29.0",
    "@tsoa/cli": "^3.5.2",
    "@types/aws-lambda": "^8.10.72",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.11",
    "@types/node": "^12.19.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/url-join": "^4.0.0",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "serverless": "^2.29.0",
    "serverless-offline": "^6.8.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-webpack": "^5.4.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-node-externals": "^2.5.2"
  },
